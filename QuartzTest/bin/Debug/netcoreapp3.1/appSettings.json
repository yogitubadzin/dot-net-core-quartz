{
  "Scheduling": {
    "StartProcessOption": {
      "StartPaymentsJobOption": "2",
      "StartInvoicesJobOption": "2"
    },
    "CronExpression": {
      "PaymentsJob": "0 0/1 * 1/1 * ? *",
      "InvoicesJob": "15 0/1 * 1/1 * ? *"
    }
  },
  "WindowsServiceSettings": {
    "RecoverServiceDelayInSeconds": "60"
  }
}